package com.nit.skyio;

public class Squarr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int num=4;
      for(int i=1;i<=num;i++) {
    	  for(int j=1;j<=num;j++) {
    		  System.out.print("* ");
    	  }
    	  System.out.println("  ");
      }
      
	}

}
